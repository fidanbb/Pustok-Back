@model Setting

@{
    ViewData["Title"] = "Detail";
}



<div class="container mt-5 " style="margin-left:30px">


    @if (Model.Value.Contains("jpg") || Model.Value.Contains("png") || Model.Value.Contains("jpeg") || Model.Value.Contains("webp"))
    {
        <h3 class="mb-2">@Model.Key</h3>
        <img class="mt-3" src="~/assets/images/@Model.Value" />
    }
    else
    {
        <h3 class="mt-3"> @Model.Key - @Model.Value</h3>
    }


    <div class="text mt-3">
       
        <p class="mt-3"> CreatedDate - @Model.CreatedDate</p>

        <p class="mt-3"> UpdatedDate - @Model.UpdatedDate</p>


    </div>

    <div class="back mt-2">
        <a class="btn btn-info text-white" asp-action="Index">Back</a>
    </div>
</div>

