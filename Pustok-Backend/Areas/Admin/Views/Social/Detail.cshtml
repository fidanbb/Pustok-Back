@model SocialVM


@{
    ViewData["Title"] = "Detail";
}

@section css {
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" />
}

<div class="container mt-5" style="margin-left:30px;">
    <div class="image mb-3">
        <i style="font-size:20px;" class="@Model.IconName"></i>
    </div>
    <h2 class="mb-3">Link- <a href="@Model.Link" target="_blank">@Model.Link</a></h2>
    <p class="mb-3"> CreatedDate - @Model.CreatedDate</p>

    <p class="mb-3"> UpdatedDate - @Model.UpdatedDate</p>



    <div class="back mt-2">
        <a class="btn btn-info text-white" asp-action="Index">Back</a>
    </div>
</div>

